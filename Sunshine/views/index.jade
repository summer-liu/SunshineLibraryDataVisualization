doctype html
head
	title Sunshine Library Data Visualization
	link(rel='stylesheet',href='../stylesheets/style.css')
	
body
	script.
		var accuracy = [#{allData.problemAccuracy}];
		var time = [#{allData.averageFocusTime.slice(1)}];
		var problemData = [#{allData.problemData}];
		var focusTime = [#{allData.averageFocusTime}];

	script(src="http://d3js.org/d3.v3.min.js")
	script(src="http://labratrevenge.com/d3-tip/javascripts/d3.tip.v0.6.3.js")

	script
		include ../public/javascripts/d3chart.js
		include ../public/javascripts/d3chart3.js
		include ../public/javascripts/d3chart2.js

	table#overview
		caption 
			strong Overview
		colgroup
			col(span = "1" style = "background-color:#48cfad ; width:40% " )
		colgroup
			col(span = "1" style = "background-color:#F5FFFA ; width:60% " )
		tr
			td#hd Carriculum
			td#dd #{allData.carriculum}
		tr
			td#hd Date
			td#dd #{allData.date}
		tr
			td#hd School
			td#dd #{allData.school}
		tr 
			td#hd Description
			td#dd #{allData.description}
		tr 
			td#hd Number of Students
			td#dd  #{allData.studentNum}
		tr
			td#hd Number of Problems
			td#dd  #{allData.problemNum}
		tr
			td#hd Average Student Hours
			td#dd #{allData.averageStudentHours}
		tr
			td#hd Total Student Hours
			td#dd #{allData.totalStudentHours}

